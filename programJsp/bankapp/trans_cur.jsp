<SCRIPT>
//检查用户是否已经输入帐号和密码的函数
function checkpass()
{
	if((form.ownername.value=="")||(form.toNo.value==""))
	{
		alert("请录入完整信息!");
	}
	else if (!isValidDouble(form.amount.value))
	{
		alert("转入金额应该是一个合法的实数！");
	}
	else
	{
		form.submit();
	}
}
</script>
